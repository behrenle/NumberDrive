[
  {
    "def": "sindeg(x):=sin(x*deg)",
    "deps": ["sin", "deg"]
  },
  {
    "def": "cosdeg(x):=cos(x*deg)",
    "deps": ["cos", "deg"]
  },
  {
    "def": "tandeg(x):=tan(x*deg)",
    "deps": ["tan", "deg"]
  },

  {
    "def": "asindeg(x):=asin(x)/deg",
    "deps": ["asin", "deg"]
  },
  {
    "def": "acosdeg(x):=acos(x)/deg",
    "deps": ["acos", "deg"]
  },
  {
    "def": "atandeg(x):=atan(x)/deg",
    "deps": ["atan", "deg"]
  },
  {
    "def": "normal(mu,sigma,x):=1/(sigma*sqrt(2*pi))*exp((x-mu)^2/(2*sigma))",
    "deps": ["pi", "exp"]
  }
]
